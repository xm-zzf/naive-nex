  },
  "peerDependencies": {
    "naive-ui": "^2.38.2",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "autoprefixer": "^10.4.19",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "naive-ui": "^2.38.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
